{
  "name": "@shunkakinoki/renovate-config",
  "version": "1.1.148",
  "private": false,
  "homepage": "https://github.com/shunkakinoki/configurations",
  "bugs": {
    "url": "https://github.com/shunkakinoki/configurations/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shunkakinoki/configurations.git"
  },
  "license": "MIT",
  "author": "Shun Kakinoki",
  "main": "src/index.js",
  "scripts": {
    "fix": "cd ../.. && yarn run fix"
  },
  "dependencies": {
    "renovate": "^34.158.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "renovate-config": {
    "default": {
      "automerge": true,
      "automergeType": "pr",
      "rangeStrategy": "bump",
      "prConcurrentLimit": 0,
      "prHourlyLimit": 0,
      "schedule": "every 3 days",
      "extends": [
        "config:base",
        "group:monorepos"
      ]
    },
    "shunkakinoki": {
      "packagePatterns": [
        "^@shunkakinoki/"
      ],
      "sourceUrlPrefixes": [
        "https://github.com/shunkakinoki/configurations"
      ]
    }
  }
}
